{
  "name": "dotbox-components",
  "version": "0.1.0",
  "description": "A reusable web component library with stylish buttons and more",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "build:prod": "rollup -c rollup.prod.config.js",
    "dev": "rollup -c -w",
    "serve": "node server.js",
    "start": "concurrently \"npm run dev\" \"npm run serve\""
  },
  "keywords": [
    "web-components",
    "buttons",
    "ui-components"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "concurrently": "^8.2.2",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-copy": "^3.4.0",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "lit": "^2.7.5"
  }
} 